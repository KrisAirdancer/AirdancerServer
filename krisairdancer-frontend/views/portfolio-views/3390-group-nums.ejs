<style>
    body {
        background-color: #122D4A;
        color: #FFA366;
    }

    main {
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        justify-content: center;
    }

    h1 {
        font-size: 2rem;
        text-align: center;
        margin-bottom: 5px;
    }

    p {
        margin: 0px;
        padding: 0px;
        text-align: center;
        margin-bottom: 10px;
    }

    .image {
        display: flex;
        width: 400px;
        height: 400px;
        box-shadow: 3px 3px 15px #081421;
        border-radius: 5px;
        margin: 5px;
        background-position: center;
        background-size: cover;
    }

    .nsfw-overlay {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        width: 400px;
        height: 400px;
        border-radius: 5px;
        background-color: rgb(228, 228, 228);
        content: 'NSFW';
        color: black;
    }

    .nsfw-overlay:hover {
        cursor: pointer;
    }

    .front-back-overlay {
        height: 20px;
        width: 35px;
        color: black;
        background-color: rgba(0, 0, 0, 0.493);
        border-radius: 3px;
        text-align: center;
    }

    .image-hover:hover {
        cursor: pointer;
    }

